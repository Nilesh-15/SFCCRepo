<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">

    <custom-type type-id="productBackInStock">
        <display-name xml:lang="x-default">Product BackIn Stock</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <retention-days>180</retention-days>
        <key-definition attribute-id="product_email_id">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="customerEmail">
                <display-name xml:lang="x-default">Customer Email</display-name>
                <type>email</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="productId">
                <display-name xml:lang="x-default">Product Id</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="backInStock">
                <display-name xml:lang="x-default">Back In Stock</display-name>
                <attribute attribute-id="productId"/>
                <attribute attribute-id="product_email_id"/>
                <attribute attribute-id="creationDate" system="true"/>
                <attribute attribute-id="lastModified" system="true"/>
                <attribute attribute-id="customerEmail"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>