<button type="submit" class="add-to-wish-list add-to-wish-list-pdp ${product.availability != null && product.availability.outOfStock ? 'd-none':''}" ${!product.available ? "disabled" : ""} href="${URLUtils.url('Wishlist-AddProduct')}">
    <i class="fa fa-heart-o"></i>
</button>
<button type="submit" class="add-to-wish-list remove-from-wishlist-pdp d-none ${product.availability != null && product.availability.outOfStock ? 'd-none':''}" ${!product.available ? "disabled" : ""} data-href="${URLUtils.url('Wishlist-RemoveProduct', 'pid', product.id)}">
    <i class="fa fa-heart"></i>
</button>
